/* Player Attack Monster*/
Private Sub CmdATK_OnEvent()
  /* Start the timer for Monster's attack. Until monster atack player, disable player's ATK Command */
  CmdATK.enabled = False
    PlayerDamage = PlayerATK - MonsterDFS
      if PlayerDamage = 0 or PlayerDamage < 0 then 
      PlayerDamage = 1
      End if
  /* Decrease Monster Hp and display the currrent HP as well as original HP */
  MonsterHP = MonsterHP - PlayerDamage
  lblMonsterHP.caption = "HP: " & MonsterHP & "/" & MonsterHPOriginal
  
 'Monster's attack 
  MonsterName = lblMonsterName.caption
  MonsterDamage = MonsterATK - PlayerDFS 
      if MonsterDamage = 0 or MonsterDamage < 0 then
        MonsterDamage = 1
      end if
  PlayerHP = PlayerHP - MonsterDamage
  /*Player dead*/
  if PlayerHp = 0 or PlayerHP < 0 then
  lblGameOver.visible = True
  CmdRestartF1.visible = True
  CmdRestartFX.visible = True
  cmdATK.visible = False 
  CmdPotion.visible = false
  Cmdrun.visible = false
  CmdSpell.visible = false
  CmdUltimate.visible = False
  end if
  /* Monster attack, display player's HP, enable ATK button for further player's attack */
   lblDescription.Caption  = "Player dealt " & PlayerDamage & " Damage," & MonsterName & " dealt " & MonsterDamage & " damages"
   lblHp.caption = "Health: " & PlayerHP & "/" & PlayerHPOriginal
   cmdATK.enabled = True
  
  
  
  
  /* Killing monster */
 if MonsterHP < 1 then
  /* When monster's HP reach 0 or below, then monster's HP = 0 */
    MonsterHP = 0
    lblMonsterHP.caption = "HP: " & MonsterHP & "/" & MonsterHPOriginal
    /*Randomly drop potion*/
      Potiondrop = int(5 * Rnd)
      if potiondrop = potionChance then 
        Potion = potion + 1
        lblpotion.caption = "potion: " & potion
      end if
      /*Display cross, disable attack option, and show next button */
      imgCross.visible = True
      CmdMiddle.visible = True
      CmdRight.visible = True
      CmdLeft.visible = True
      cmdATK.enabled = False 
      ATKCount = 1
      
     'Spell point up
      if lblspell4.visible = true then
        lblspell5.visible = true
        lblSpellMax.visible = True
      end if
      if lblspell3.visible = true then
        lblspell4.visible = true
      end if
      if lblspell2.visible = true then
        lblspell3.visible = true 
      end if 
      if lblspell1.visible = true then
        lblspell2.visible = true     
      end if
      if lblspell1.visible = False then 
        lblspell1.visible = True
      end if
     
  /* Give player random EXP based on monster's strength and display it */
  PlayerEXP = PlayerEXP - MonsterEXP
  lblEXP.caption = "EXP: Next level in  " &  PlayerEXP & " Pts"    
  End if
  
  
  
  
  
  /* Leveling up */
 if PlayerEXP < 1 then 
   /* generate next EXP needed to level up higher */
   PlayerEXP = 10 + Playerlevel * 4
   PlayerLevel = PlayerLevel + 1
     PlayerHPOriginal = PlayerHPOriginal + PlayerLevel * 15
     lblHp.caption = "Health: " & PlayerHP & "/" & PlayerHPOriginal
     PlayerHP = PlayerHPOriginal
       PlayerATK = PlayerATK + Playerlevel * 3
       LblATK.caption = "Attack: " & PlayerATK
         PlayerDFS = PlayerDFS + Playerlevel 
         LblDFS.caption = "Defense: " & PlayerDFS
    /* Display player's new level and how many Points until next level*/
    lblLevel.caption = "Level: " & PlayerLevel
    lblEXP.caption = "EXP: Next level in  " &  PlayerEXP & " Pts"
    lblDescription.caption = "You leveled up to " & Playerlevel
  End if 
  
End Sub





Private Sub CmdPotion_OnEvent()
  'can't use potion if there aren't any
  if potion = 0 then 
    lblDescription.caption = "You have no potion"
  end if
  
