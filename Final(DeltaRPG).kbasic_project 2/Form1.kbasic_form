  if potion > 0 then 
  Potion = potion - 1
  lblPotion.caption = "Potion:" & potion
  PlayerHP = PlayerHPOriginal
  lblHp.caption = "Health: " & PlayerHP & "/" & PlayerHPOriginal
  end if
