Private Sub Form_OnKeyDown(KeyCode As Integer, Shift As Boolean, Control As Boolean, Alt As Boolean)
  'Attack, Spell, Ultimate move, select path (right, left, middle), Run, and Use potion
  
  'Cheat code
      If KeyCode = Key.K Then
        PlayerATK = PlayerATK + 200
        lblATK.caption = "Attack: " & PlayerATK
        PlayerDFS = PlayerDFS + 200
        lblDFS.caption = "Defense: " & PlayerDFS
        PlayerHP = PlayerHP + 1000
        PlayerHPOriginal = PlayerHPOriginal + 1000
        lblHP.caption = "Health: " & PlayerHP & "/" & PlayerHPOriginal
        
    End if
  'Attack key
      If KeyCode = Key.A Then
        if lblGameover.visible = False and imgcross.visible = False then
        CmdATK_OnEvent()
    End If
    End if
  'Potion key
      If KeyCode = Key.P Then
        if lblGameover.visible = False and imgcross.visible = False then
        CmdPotion_OnEvent()
    End If 
    End if        
  'Ultimate key
      If KeyCode = Key.U Then
        if lblGameover.visible = False and imgcross.visible = False and lblUltimateMax.visible = True then
        CmdUltimate_OnEvent()
    End If  
    End if
  'Spell key
    If KeyCode = Key.S Then
       if lblGameover.visible = False and imgcross.visible = False and lblSpellMax.visible = True then
        CmdSpell_OnEvent()
    End if
    End if
  'Select Right path 
        If KeyCode = Key.Right Then
    if CmdRight.visible = True then
        CmdRight_OnEvent()
    end if
    End If
  
  'Select middle path
    If KeyCode = Key.Up Then
    if CmdMiddle.visible = True then
        CmdMiddle_OnEvent()
    End If
    end if

  'Select Left path
    If KeyCode = Key.Left Then
    if Cmdleft.visible = True then
        CmdLeft_OnEvent()
    End If 
    End if

End Sub 


Private Sub Form_OnOpen()
/* Player's name */
PlayerName = inputbox("What is your name?") 
lblName.caption =  PlayerName
/* Player's original stats */
lblHP.caption = "HP: 15/15 "
lblATK.caption = "Attack: 5"
lblDFS.caption = "Defense: 2"
PlayerHP = 15
Reply = msgbox("Would you like to have tutorial before proceeding?",kbretrycancel,"")
if reply = 1 then 
msgbox("The objective of this game is to defeat monsters on each level and eventually defeat BOSS to clear the game.")
msgbox("You may use either attack, spell, potion, run, or Ultimate move to fight monsters.")
msgbox("You can use <, ^, or >, key on computer to select path between left, middle, or right")
msgbox("Press A for attack, P for potion, S for spell, U for Ultimate move, and R for run")
msgbox("there is also a cheat key somewhere, find it and you will be rewarded with fortune.")
msgbox("Thats all you need to know for now, good luck with this journey to this DeltaRPG. Enjoy!")
End if
End Sub




Private Sub CmdMiddle_OnEvent()
  cmdpotion.visible = True
  cmdATK.visible = True
  cmdRun.visible = True
  'display spells and moves if guage is full
  If LblSpellMax.visible = True then
    CmdSpell.visible = True
  end if  
  If LblUltimateMax.visible = true then 
    cmdUltimate.visible = true
  end if
  
  Path = int(10 * Rnd) 
  lblDebug.caption = path
  imgpotion.visible = False

'drop potion at 30% chance
if path = 0 or 1 or 2 then 
  potion = potion + 1
  Lbldescription.caption = "You got a Potion"
  lblpotion.caption = "Potion: " & potion
  Imgpotion.visible = true
end if


/* make this button invisible and enable ATK command for player to attack next monster */
CmdMiddle.visible = False
CmdRight.visible = False
CmdLeft.visible = False
CmdATK.enabled = True
ImgCross.visible = False
 Floor = Floor + 1
imgpotion.visible = False 
imgmonster.visible = True
 
 /*weak 3 on each floor*/
 /*1-2*/
 If Floor > 0 and floor < 3 then floorlevel = 1
 /*3-5*/
 If Floor > 2 and floor < 6 then floorlevel = 2
 /*6-8*/
 If Floor > 5 and floor < 9 then floorlevel = 3
 /*9-11*/
 If Floor > 8 and floor < 12 then floorlevel = 4
 /*12-15*/
 If Floor > 11 and floor < 16 then floorlevel = 5
/*Medium 4 on each floor*/
 /*16-19*/
 If Floor > 15 and floor < 20 then floorlevel = 6
 /*20-23*/
 If Floor > 19 and floor < 24 then floorlevel = 7
 /*24-27*/
 If Floor > 23 and floor < 28 then floorlevel = 8
 /*28-31*/
 If Floor > 27 and floor < 32 then floorlevel = 9
 /*32-35*/
 If Floor > 31 and floor < 36 then floorlevel = 10
 /*Hard 5 on each floor*/
 /*36-40*/
 If Floor > 35 and floor < 41 then floorlevel = 11
